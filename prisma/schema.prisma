generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-arm64-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Paste {
  id        String   @id @default(uuid())
  title     String
  content   String
  createdAt DateTime @default(now())
} 